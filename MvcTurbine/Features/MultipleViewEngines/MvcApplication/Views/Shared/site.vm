<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title>NVelocity View Engine</title>
    <link href="../../Content/Site.css" rel="stylesheet" type="text/css" />
</head>
<body>
    <div class="page">
        <div id="header">
            <div id="title">
                <h1>My MVC Application powered by MVC Turbine</h1>
            </div>
            <div id="logindisplay">
                &nbsp;
            </div>
            <div id="menucontainer">
                <ul id="menu">
                    <li>
                        $Html.ActionLink("WebForm", "Index", "Home")</li>
                    <li>
                        $Html.ActionLink("Spark", "Index", "Spark")</li>
                    <li>
                        $Html.ActionLink("NVelocity", "Index", "NVelocity")</li>
                    <li>
                       $Html.ActionLink("Mixed", "Index", "Mixed")</li>
                </ul>
            </div>
        </div>
        <div id="main">
            #parse($childContent)
            <div id="footer">
                <p>powered by <a href="http://mvcturbine.codeplex.com">MVC Turbine</a></p>
            </div>
        </div>
    </div>
</body>
</html>
